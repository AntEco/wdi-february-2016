this is the users index page

<ul>
<% @users.each do |user| %>
  <li>
    <strong>Full Name: </strong><%= user.full_name %><br />
    <strong>Email: </strong><%= user.email %><br />
    <strong>Password: </strong><%= user.password %><br />
    <%= link_to "Delete User", user_path(user.id), method: :delete %>
    
    <!-- /users/4 == user_path(4) -->
  </li>
<% end %>
</ul>