<%= form_tag("/new_search", method: :get) do %>
  <%= label_tag(:query, "Search:") %>
  <%= text_field_tag(:query) %>
  <%= submit_tag("Submit") %>
<% end %>

<%= form_tag("/login", method: :post) do %>
  <%= label_tag(:email) %>
  <%= text_field_tag(:email) %>
  <%= label_tag(:password) %>
  <%= text_field_tag(:password) %>
  <%= submit_tag("Submit") %>
<% end %>